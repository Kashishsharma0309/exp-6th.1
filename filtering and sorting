import java.util.*;
import java.util.stream.*;

class Student {
    String name;
    double marks;

    Student(String name, double marks) {
        this.name = name;
        this.marks = marks;
    }

    @Override
    public String toString() {
        return name + " - Marks: " + marks;
    }
}

public class StudentStreamExample {
    public static void main(String[] args) {
        List<Student> students = Arrays.asList(
                new Student("Kashish", 85),
                new Student("Aarav", 72),
                new Student("Riya", 91),
                new Student("Mohan", 65),
                new Student("Tina", 88)
        );

        System.out.println("Students scoring above 75%, sorted by marks:");

        students.stream()
                .filter(s -> s.marks > 75)                // Filter students with marks > 75
                .sorted((s1, s2) -> Double.compare(s2.marks, s1.marks)) // Sort by marks (descending)
                .map(s -> s.name)                         // Extract only names
                .forEach(System.out::println);            // Display names
    }
}
